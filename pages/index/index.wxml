<!--index.wxml-->
<view  wx:for="{{orderList}}" wx:for-index="idx" wx:key="oid" wx:for-item="item">
  <view class="item" bindtap="toDetail" data-oid="{{item.o_id}}">
    <view class="row">
      <view style="margin-left:10px;">{{item.consignee_range_time}}</view>
      <view style="color:red;margin-left:auto;margin-right:10px;">{{item.chargeback_status==1?'退单待处理':''}}</view>
    </view>
    <view style="margin-top:5px;" class="row" >
      <view style="margin-left:10px;">{{item.consignee_name}}</view>
      <view style="margin-left:5px;">{{item.consignee_phone}}</view>
    </view>
    <view style="margin-left:10px;margin-top:5px;margin-bottom:5px;overflow:hidden;text-overflow:ellipsis;white-space:nowrap;width:80%">{{item.consignee_address}}</view>
  </view>
</view> 