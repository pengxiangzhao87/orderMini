/**index.wxss**/
page{
  width:100%;
  height:100%;
  background-color:rgb(248, 248, 248)ï¼›
}
.item{
  color:white;
  background-color:#1296db;
  border-radius: 10px;
  margin:5px 5px 0 5px;
  font-size:14px; 
  height:100%;
  padding-top:5px;
}

.row{
  display: flex;  
  flex-direction: row  ;
  align-items: center;
  
}

.title{
  display: flex;  
  flex-direction: row ;
  align-items: center;
  margin-top:15px;
  margin-bottom: 15px;
  color:#333333;
  font-size:14px;
}
.title-item{
  display:flex;
  align-items: center;
  justify-content: center;
  color:#666666;
  width:33%;
  font-weight:500;
  margin-left:12%;
}

.title-item.checked{
  display:flex;
  align-items: center;
  justify-content: center;
  color:black;
  width:33%;
  font-weight:500;
  margin-left:12%;
 
}

.big.checked{
  padding-bottom:2px;
  border-bottom: 2px solid #1296db ;
}

.btn{
  color:white;
  margin-left:auto;
  margin-right:10px;
  border:1px solid white;
  background-color:#1296db;
  width:80px;
  height:25px;
  border-radius:5px;
  display: flex;
  align-items: center;
  justify-content:center;
}